package model;

public class Produto {
	// atributos
	private int id;
	private String nome;
	private String descricao;
	private double preco;
	
	// contrutor vazio
	public Produto() {
		
	}
	
	// contrutor cheio
	public Produto(int id, String nome, String descricao, double preco) {
		this.id = id;
		this.nome = nome;
		this.descricao = descricao;
		this.preco = preco;
	}
	
	
	
	
	
	
}
