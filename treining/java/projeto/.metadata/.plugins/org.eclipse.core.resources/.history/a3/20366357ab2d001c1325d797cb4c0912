package model;

public class Cliente {
	// atributos
	private int id;
	private String cpf;
	private String nome;
	private String email;
	private String senha;
	private String endereco;
	
	// construtor vazio
	public Cliente() {
		
	}

	// contrutor cheio
	public Cliente(int id, String cpf, String nome, String email, String senha, String endereco) {
		this.id = id;
		this.cpf = cpf;
		this.nome = nome;
		this.email = email;
		this.senha = senha;
		this.endereco = endereco;
	}
	
	
	
	
	
}
